name: mlflow-project-example

conda_env: env.yaml

entry_points:
  main:
    parameters:
      path: {type: string, default: "data/sf-airbnb-clean.parquet"}
      maxBins: {type: float, default: 40}
      maxDepth: {type: float, default: 5}
      numTrees: {type: float, default: 100}
    command: "python tracking.py {path} {maxBins} {maxDepth} {numTrees}"